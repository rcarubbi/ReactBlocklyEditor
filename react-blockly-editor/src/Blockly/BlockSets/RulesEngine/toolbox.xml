<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
  <category name="Rules" colour="#a55b80">
    <block type="rule_expression"></block>
    <block type="rule_expression_group"></block>
    <block type="rule_expression_sequence">
      <statement name="expression_statements">
        <block type="rule_sequence_item">
          <field name="rule_name">Name</field>
        </block>
      </statement>
    </block>
  </category>
  <category name="Entities" colour="#5ba55b">
    <block type="rfl">
      <field name="property_name">Property</field>
      <field name="entity_name">Entity</field>
    </block>
    <block type="rfl_instrument">
      <field name="property_name">Property</field>
    </block>
    <block type="rfl_business_date"></block>
    <block type="rfl_issuer">
      <field name="property_name">Property</field>
    </block>
    <block type="rfl_portfolio">
      <field name="property_name">Property</field>
    </block>
    <block type="rfl_position_definition">
      <field name="property_name">Property</field>
    </block>
    <block type="with_safe_check"></block>
  </category>
  <sep></sep>
  <category name="Logic" colour="#5b80a5">
    <block type="logic_compare">
      <field name="OP">EQ</field>
    </block>
    <block type="logic_operation">
      <field name="OP">AND</field>
    </block>
    <block type="logic_negate"></block>
    <block type="logic_boolean">
      <field name="BOOL">TRUE</field>
    </block>
    <block type="logic_null"></block>
    <block type="logic_ternary"></block>
  </category>
  <category name="Text" colour="#5ba58c">
    <block type="text">
      <field name="TEXT"></field>
    </block>
  </category>
</xml>